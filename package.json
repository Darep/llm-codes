  },
  "homepage": "https://llm.codes/",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.21",
    "next": "^15.3.3",
    "postcss": "^8.5.5",
